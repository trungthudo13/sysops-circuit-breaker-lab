    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver"
            ],
            "console": "integratedTerminal",
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/src/manage.py",
            "justMyCode": false,
            "env": {
                "DEBUG": "True",
            }
        },
        {
            "name": "Python Debugger: Frontier",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver",
                "8001"
            ],
            "console": "integratedTerminal",
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/src/manage.py",
            "justMyCode": false,
            "env": {
                "DEBUG": "True",
                "EXT_INSTALLED_APPS": "apps.frontier",
                "FRONTIER_DOMAIN": "http://127.0.0.1:8001",
                // "MIDLINE_DOMAIN": "http://127.0.0.1:8002",
                // "BACKLINE_DOMAIN": "http://127.0.0.1:8003"
                "MIDLINE_DOMAIN": "http://127.0.0.1:8003"
            }
        },
        {
            "name": "Python Debugger: Midline",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver",
                "8002"
            ],
            "console": "integratedTerminal",
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/src/manage.py",
            "justMyCode": false,
            "env": {
                "DEBUG": "True",
                "EXT_INSTALLED_APPS": "apps.midline",
                "FRONTIER_DOMAIN": "http://127.0.0.1:8001",
                "MIDLINE_DOMAIN": "http://127.0.0.1:8002",
                "BACKLINE_DOMAIN": "http://127.0.0.1:8003"
            }
        },
        {
            "name": "Python Debugger: Backline",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver",
                "8003"
            ],
            "console": "integratedTerminal",
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/src/manage.py",
            "justMyCode": false,
            "env": {
                "DEBUG": "True",
                "EXT_INSTALLED_APPS": "apps.backline",
                "FRONTIER_DOMAIN": "http://127.0.0.1:8001",
                "MIDLINE_DOMAIN": "http://127.0.0.1:8002",
                "BACKLINE_DOMAIN": "http://127.0.0.1:8003"
            }
        },
        {
            "name": "Python Debugger: Granian",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "--interface",
                "wsgi",
                "--workers",
                "1",
                "--host",
                "0.0.0.0",
                "--access-log",
                "InsightTrack.wsgi:application"
            ],
            "console": "integratedTerminal",
            "django": true,
            "autoStartBrowser": false,
            "program": "/Users/trungthudo/.pyenv/versions/hifpt_visualize/bin/granian",
            "justMyCode": false,
            "env": {
                "DEBUG": "True",
            }
        },
    ]
}